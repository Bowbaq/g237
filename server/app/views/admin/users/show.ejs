<div class="page-header">
    <h1>Details of user</h1>
</div>


<table class="table table-bordered">
    <tbody>
        <tr><th>Username</th><td><%= user.username %></td></tr>
        <tr><th>Hash</th><td><%= user.hash %></td></tr>
        <tr><th>Salt</th><td><%= user.salt %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', path_to.edit_admin_user(user), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', path_to.admin_user(user), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', path_to.admin_users()) %>
</div>